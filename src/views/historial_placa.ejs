<h2>Historial — <%= placa %></h2>
<table>
  <thead><tr><th>ID</th><th>CEDIS</th><th>Tipo</th><th>Inicio</th><th>Fin</th></tr></thead>
  <tbody>
  <% if (!items || !items.length) { %>
    <tr><td colspan="5">Sin historial</td></tr>
  <% } else { items.forEach(m => { %>
    <tr>
      <td><%= m.id %></td>
      <td><%= m.cedis_nombre || 'N/D' %></td>
      <td><%= m.tipo %></td>
      <td><%= dayjs(m.fecha_inicio).format('YYYY-MM-DD') %></td>
      <td><%= m.fecha_fin ? dayjs(m.fecha_fin).format('YYYY-MM-DD') : '—' %></td>
    </tr>
  <% }) } %>
  </tbody>
</table>
