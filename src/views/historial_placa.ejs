<section class="card">
  <h1>Historial â€¢ <%= placa %></h1>
  <table>
    <thead>
      <tr><th>ID</th><th>Tipo</th><th>Inicio</th><th>Fin</th><th>Motivo</th></tr>
    </thead>
    <tbody>
    <% items.forEach(m => { %>
      <tr>
        <td><%= m.id %></td>
        <td><%= m.tipo %></td>
        <td><%= m.fecha_inicio ? dayjs(m.fecha_inicio).format('DD/MM/YYYY') : '' %></td>
        <td><%= m.fecha_fin ? dayjs(m.fecha_fin).format('DD/MM/YYYY') : '' %></td>
        <td><%= m.motivo || '' %></td>
      </tr>
    <% }) %>
    </tbody>
  </table>
</section>
